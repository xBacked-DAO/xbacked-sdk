{
  "name": "@xbacked-dao/xbacked-sdk",
  "version": "0.0.1",
  "description": "Official SDK for the xBacked Protocol",
  "main": "lib/cjs/index.js",
  "module": "/lib/esm/index.js",
  "types": "lib/index.d.ts",
  "packageManager": "yarn@3.1.0",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "yarn run build-esm & yarn run build-cjs",
    "build-esm": "tsc -p ./tsconfig.json",
    "build-cjs": "tsc -p ./tsconfig-cjs.json",
    "format": "prettier \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xBacked-DAO/xbacked-sdk.git"
  },
  "keywords": [],
  "author": "Xbacked",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xBacked-DAO/xbacked-sdk/issues"
  },
  "homepage": "https://github.com/xBacked-DAO/xbacked-sdk#readme",
  "devDependencies": {
    "@jest/globals": "^27.4.6",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.9",
    "eslint": "^8.4.1",
    "eslint-config-google": "^0.14.0",
    "jest": "27.3.1",
    "jest-environment-jsdom": "^27.0.6",
    "lodash": "^4.17.21",
    "prettier": "^2.4.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@babel/types": "^7.16.0",
    "@randlabs/myalgo-connect": "^1.1.1",
    "@reach-sh/stdlib": "^0.1.8-rc.1",
    "@types/jsdom": "^16.2.13",
    "@xbacked-dao/xbacked-contracts": "^0.0.6",
    "awesome-typescript-loader": "^5.2.1",
    "jest": "^27.3.1"
  }
}
